{
  "scripts": {
    "start": "node ./bin/www",
    "build": "webpack -p --config webpack.prod.config.js",
    "build:dev": "webpack --config webpack.dev.config.js --progress",
    "dev": "webpack-dev-server --config webpack.dev.config.js --progress",
    "format":
      "prettier --write --single-quote --print-width=120 --parser=flow \"{src/js,app_api,app_server}/**/*.{js,jsx}\"",
    "lint": "eslint client/**/*.{js,jsx} --quiet",
    "watch": "webpack --watch --config webpack.dev.config.js",
    "test": "jest --coverage",
    "test:dd": "jest --watch",
    "heroku-postbuild": "npm run build"
  },
